{
  "name": "@tgl/core",
  "version": "0.2.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/mode777/tgl"
  },
  "description": "A small webgl library for typescript",
  "private": false,
  "keywords": [
    "webgl",
    "typescript"
  ],
